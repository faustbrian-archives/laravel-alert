{
    "name": "artisanry/alert",
    "description": "Alert messages for Laravel 5.",
    "keywords": [
        "laravel",
        "framework",
        "alert",
        "flash"
    ],
    "license": "MPL-2.0",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@basecode.sh",
        "homepage": "https://basecode.sh"
    }],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^5.8|^6.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Artisanry\\Alert\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Artisanry\\Tests\\Alert\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "Artisanry\\Alert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "Artisanry\\Alert\\Facades\\Alert"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
