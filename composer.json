{
    "name": "faustbrian/laravel-alert",
    "description": "Alert messages for Laravel 5.",
    "keywords": ["laravel", "framework", "alert", "flash"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.me",
        "homepage": "https://brianfaust.me"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "graham-campbell/testbench": "^4.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Alert\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Alert\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "BrianFaust\\Alert\\AlertServiceProvider"
            ],
            "aliases": {
                "Alert": "BrianFaust\\Alert\\Facades\\Alert"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
